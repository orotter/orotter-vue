{
  "name": "orotter-vue",
  "version": "1.0.0",
  "author": "ktsn <ktsn55@gmail.com>",
  "description": "Awesome microblog service",
  "private": true,
  "scripts": {
    "clean": "rm -rf .tmp dist",
    "build": "webpack --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:test": "webpack --config webpack.config.test.js",
    "watch": "webpack -w --config webpack.config.js",
    "watch:test": "webpack -w --config webpack.config.test.js",
    "release": "run-s test:ci clean build:prod",
    "lint": "eslint --ext js,vue src test",
    "lint:fix": "npm run lint -- --fix",
    "dev": "webpack-dev-server --inline --hot --quiet --config webpack.config.js",
    "testem": "testem",
    "testem:ci": "testem ci --launch Firefox",
    "test": "run-p watch:test testem",
    "test:ci": "run-s lint build:test testem:ci"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-unassert": "^2.1.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-power-assert": "^1.0.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.1",
    "eslint-plugin-html": "^1.7.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.26.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.3.0",
    "npm-run-all": "^4.0.0",
    "postcss-loader": "^1.2.2",
    "power-assert": "^1.4.2",
    "pug": "^2.0.0-beta6",
    "sass-loader": "^4.1.1",
    "sinon": "^2.0.0-pre.2",
    "style-loader": "^0.13.1",
    "testem": "^1.14.2",
    "vue-loader": "^10.0.2",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^1.14.0",
    "webpack-dashboard": "^0.2.1",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "k-css": "^1.0.0",
    "lodash": "^4.17.4",
    "vue": "^2.1.10",
    "vue-router": "^2.1.1",
    "vuex": "^2.1.1",
    "vuex-router-sync": "^4.1.0",
    "vuex-toast": "0.1.1"
  }
}
